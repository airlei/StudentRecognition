<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.projects.modular.attendance.mapper.ClassRoomMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.projects.modular.attendance.entity.ClassRoom">
        <id column="room_id" property="roomId" />
        <result column="name" property="name" />
        <result column="start_time" property="startTime" />
        <result column="end_time" property="endTime" />
        <result column="user_id" property="userId" />
        <result column="classes_id" property="classesId" />
        <result column="demand" property="demand" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        room_id AS "roomId", name AS "name", start_time AS "startTime", end_time AS "endTime", user_id AS "userId", classes_id AS "classesId", demand AS "demand"
    </sql>

</mapper>
